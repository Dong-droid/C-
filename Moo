#include <iostream>
using namespace std;
int N;
int S[30];//moo 0903 분할정복, 재귀
void sol(int i){
//	cout<<N<<'\n';
	if(i==0){
	if(N==1) cout<<'m';
	else if(N==2||N==3)cout<<'o';
	exit(0);
	}
	
	if(N<=S[i-1]) {
		sol(i-1);
	}
    if(N>S[i]-S[i-1]){
		N-=S[i]-S[i-1];
		sol(i-1);
	}
	else {
		if(N==S[i-1]+1) cout<<'m';
		else cout<<'o';
		exit(0);
	}
}
int main() {
	cin>>N;
	S[0]=3;
	int start=0;
	for(int i=1;i<30;i++){
		S[i]=2*S[i-1]+i+3;
		if(S[i]==N) {cout<<'o';return 0;}
		else if(S[i]>N){
		start=i;
		break;
		}
	}
//	cout<<start<<'\n';
	sol(start);
	return 0;
}
