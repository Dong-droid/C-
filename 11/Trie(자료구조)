#include <iostream>
using namespace std;
struct Trie{
	Trie* ch[26];
	bool end;
	Trie()
	{
	end=false;
	for(int i=0;i<26;i++) ch[i]=NULL;
	} //generator
	~Trie()
	{
	for(int i=0;i<26;i++) if(ch[i]) delete ch[i];
	}//deletor
	
	void insert(const char* s)
	{
		if(!*s){
			this->end=true;
			return;
		}
		int now=*s-'a';
		if(!ch[now]) ch[now]=new Trie;
		ch[now]->insert(s+1);
	}
	bool find(const char* s)
	{
		if(!*s){
			if(end) return true;
			return false;
		}
		int now=*s-'a';
		if(ch[now]==NULL) return false;
		return ch[now]->find(s+1);
		}
};
int main(){
	int N,M; cin>>N>>M;
	Trie* root=new Trie;
	while(N--){
	string s; cin>>s;
	//cout<<s<<'\n';
	root->insert(s.c_str());
	}
	int ans=0;
	while(M--){
		string s;cin>>s;
	//	cout<<s<<'\n';
		if(root->find(s.c_str())) ans++; 
	}
	delete root;
	cout<<ans;
}
